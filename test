    { id: 72, subject: "Mathematics", type: "INTEGER", ans: "1", text: "Let \\(f(\\theta) = \\sin \\theta + \\int_{-\\pi/2}^{\\pi/2} (\\sin \\theta + t \\cos \\theta) f(t) dt\\). Then the value of \\(\\left| \\int_0^{\\pi/2} f(\\theta) d\\theta \\right|\\) is", opts: {} },
    { id: 73, subject: "Mathematics", type: "INTEGER", ans: "100", text: "A number is called a palindrome if it reads the same backward as well as forward. For example 285582 is a six digit palindrome. The number of six digit palindromes, which are divisible by 55, is", opts: {} },
    { id: 74, subject: "Mathematics", type: "INTEGER", ans: "1", text: "If \\([\\cdot]\\) represents the greatest integer function, then the value of \\(\\int_0^{\\sqrt{\\pi/2}} [[x^2] - \\cos x] dx\\) is", opts: {} },
    { id: 75, subject: "Mathematics", type: "INTEGER", ans: "210", text: "The term independent of \\(x\\) in the expansion of \\(\\left( \\frac{x+1}{x^{2/3} - x^{1/3} + 1} - \\frac{x-1}{x - x^{1/2}} \\right)^{10}\\), where \\(x \\neq 0, 1\\) is equal to", opts: {} }
      
];

            // --- LOGIC STARTS HERE (Paste this below your questions array) ---

    // Global Variables
    let currentSubjectIndex = 0;
    let currentQuestionIndex = 0;
    let questions = []; 
    let responses = {}; 
    let status = {}; 
    let timerTime = 180 * 60; // 3 HOURS
    let interval;
    let examActive = false;

    // Login Function
    function login() {
        // Password check
        if (document.getElementById('pass').value === "Ruhvaan") {
            examActive = true;
            document.getElementById('login-screen').style.display = 'none';
            document.getElementById('test-ui').style.display = 'flex';
            
            // Try fullscreen
            try {
                document.documentElement.requestFullscreen().catch(e => console.log("Fullscreen blocked"));
            } catch(err) {}
            
            initStatus();
            switchSubject(0); // Start with Physics
            
            // Timer Start
            interval = setInterval(() => {
                timerTime--;
                let h = Math.floor(timerTime / 3600).toString().padStart(2, '0');
                let m = Math.floor((timerTime % 3600) / 60).toString().padStart(2, '0');
                let s = (timerTime % 60).toString().padStart(2, '0');
                document.getElementById('timer').innerText = `${h}:${m}:${s}`;
                if (timerTime <= 0) submitTest(true);
            }, 1000);
        } else {
            alert("Incorrect Password!");
        }
    }
    
    // Initialize Status
    function initStatus() {
        if(typeof allQuestions !== 'undefined') {
            allQuestions.forEach(q => { status[q.id] = 'not-visited'; });
        }
    }
    
    // Switch Subject
    function switchSubject(subjectIndex) {
        currentSubjectIndex = subjectIndex;
        const subjectName = ["Physics", "Chemistry", "Mathematics"][subjectIndex]; // Maths spelling fix
        
        if (typeof allQuestions !== 'undefined') {
             questions = allQuestions.filter(q => {
                // Handle Maths/Mathematics spelling
                if (subjectIndex === 2) return q.subject === "Maths" || q.subject === "Mathematics";
                return q.subject === subjectName;
            });
        }
        
        // Update Tabs
        document.querySelectorAll('.sub-tab').forEach((tab, i) => {
            if(tab) tab.classList.toggle('active', i === subjectIndex);
        });
        
        initPalette();
        if(questions.length > 0) loadQ(0);
        else alert("No questions found for " + subjectName);
    }

    // Palette
    function initPalette() {
        const p = document.getElementById('palette');
        if(p) {
            p.innerHTML = '';
            questions.forEach((q, i) => {
                const btn = document.createElement('div');
                btn.className = 'p-btn';
                btn.innerText = i + 1;
                btn.id = `pal-${q.id}`;
                btn.onclick = () => loadQ(i);
                p.appendChild(btn);
            });
            updatePalette();
        }
    }

    // --- MAIN LOAD QUESTION FUNCTION (FIXED) ---
    function loadQ(idx) {
        currentQuestionIndex = idx;
        const q = questions[idx];
        if(!q) return;
        
        // Status update
        if (status[q.id] === 'not-visited') status[q.id] = 'visited';
        
        // Update Header
        const qNumEl = document.getElementById('q-num');
        const qTypeEl = document.getElementById('q-type');
        if(qNumEl) qNumEl.innerText = `Question ${idx + 1}`;
        if(qTypeEl) qTypeEl.innerText = q.type;
        
        // --- 1. QUESTION IMAGE LOGIC ---
        const imgContainer = document.getElementById('q-img-container');
        if(imgContainer) imgContainer.innerHTML = ''; 
        
        let displayText = q.text;
        let qImageUrl = q.img || "";

        // Text me se [link] dhundo
        const linkMatch = displayText.match(/\[(https?:\/\/[^\]]+)\]/);
        if (linkMatch) {
            qImageUrl = linkMatch[1];
            displayText = displayText.replace(linkMatch[0], "").trim();
        }

        // Image dikhao
        if (qImageUrl && imgContainer) {
            const img = document.createElement('img');
            img.src = qImageUrl;
            img.className = 'q-img';
            img.style.maxWidth = "100%";
            img.style.maxHeight = "350px";
            img.style.border = "1px solid #ddd";
            img.style.display = "block";
            img.style.margin = "10px auto";
            imgContainer.appendChild(img);
        }

        document.getElementById('q-content').innerHTML = displayText;
        
        // --- 2. OPTIONS LOGIC ---
        const optsDiv = document.getElementById('options-area');
        optsDiv.innerHTML = '';
        const saved = responses[q.id] || '';

        if (q.type === 'MCQ') {
            if(q.opts) {
                Object.entries(q.opts).forEach(([key, val]) => {
                    const checked = saved === key ? 'checked' : '';
                    
                    // Option Text aur Image alag karo
                    let optText = val;
                    let optImgHTML = "";
                    
                    const optLinkMatch = val.match(/\[(https?:\/\/[^\]]+)\]/);
                    if (optLinkMatch) {
                        const optImgUrl = optLinkMatch[1];
                        optText = val.replace(optLinkMatch[0], "").trim();
                        optImgHTML = `<div style="margin-top:8px;"><img src="${optImgUrl}" style="max-height:150px; max-width:100%; border:1px solid #ccc; border-radius:4px;"></div>`;
                    }

                    optsDiv.innerHTML += `
                    <label class="option-item" style="align-items: flex-start;">
                        <div style="padding-top:2px;"><input type="radio" name="opt" value="${key}" ${checked}></div>
                        <div style="margin-left: 10px; width:100%;">
                            <strong>(${key})</strong> ${optText}
                            ${optImgHTML}
                        </div>
                    </label>`;
                });
            }
        } else {
            // Integer Type
            optsDiv.innerHTML = `
            <div class="integer-box">
                <label>Ans.</label>
                <input type="text" id="int-input" value="${saved}">
            </div>`;
        }

        if (window.MathJax) MathJax.typesetPromise();
        updatePalette();
    }

    // Save & Next
    function saveNext() {
        const q = questions[currentQuestionIndex];
        let val = null;
        if (q.type === 'MCQ') {
            const el = document.querySelector('input[name="opt"]:checked');
            if (el) val = el.value;
        } else {
            const inputEl = document.getElementById('int-input');
            if (inputEl) val = inputEl.value.trim();
        }

        if (val) {
            responses[q.id] = val;
            if (status[q.id] !== 'marked') status[q.id] = 'answered';
        }
        nav(1);
    }

    // Mark for Review
    function markReview() {
        const q = questions[currentQuestionIndex];
        status[q.id] = 'marked';
        saveNext();
    }

    // Clear Answer
    function clearAns() {
        const q = questions[currentQuestionIndex];
        delete responses[q.id];
        status[q.id] = 'visited';
        loadQ(currentQuestionIndex);
    }

    // Navigation
    function nav(dir) {
        const next = currentQuestionIndex + dir;
        if (next >= 0 && next < questions.length) {
            loadQ(next);
        } else {
            updatePalette();
            // Optional: alert("Section End");
        }
    }

    // Update Palette Colors
    function updatePalette() {
        questions.forEach((q) => {
            const btn = document.getElementById(`pal-${q.id}`);
            if(btn) {
                btn.className = 'p-btn';
                if (q.id === questions[currentQuestionIndex].id) btn.classList.add('current');
                
                const s = status[q.id];
                if (s === 'answered') btn.classList.add('answered');
                else if (s === 'marked') btn.classList.add('marked');
                else if (s === 'visited') btn.classList.add('not-ans');
            }
        });
    }

    // Submit Test
    function submitTest(force = false) {
        if (!force && !confirm("Are you sure you want to submit?")) return;
        examActive = false;
        clearInterval(interval);
        document.getElementById('test-ui').style.display = 'none';
        document.getElementById('score-card').style.display = 'flex';
        try { document.exitFullscreen().catch(e=>{}); } catch(e){}

        let correct = 0, wrong = 0, attempted = 0;
        
        // Calculate Score across ALL questions
        if(typeof allQuestions !== 'undefined') {
            allQuestions.forEach(q => {
                const userAns = responses[q.id];
                if (userAns) {
                    attempted++;
                    if (userAns.toString().trim().toLowerCase() === q.ans.toString().trim().toLowerCase()) correct++;
                    else if (q.type === 'MCQ') wrong++;
                }
            });
        }

        const totalScore = (correct * 4) - (wrong * 1);
        const maxScore = (typeof allQuestions !== 'undefined') ? allQuestions.length * 4 : 300;
        const accuracy = attempted ? ((correct / attempted) * 100).toFixed(1) : 0;

        document.getElementById('final-score').innerText = totalScore;
        document.getElementById('correct-count').innerText = correct;
        document.getElementById('wrong-count').innerText = wrong;
        document.getElementById('attempt-count').innerText = attempted;
        document.getElementById('accuracy').innerText = accuracy + '%';
    }

    // Anti-cheat
    document.addEventListener('visibilitychange', () => {
        if (document.hidden && examActive) alert("WARNING: Tab switch detected! Your test is monitored.");
    });
    </script>
</body>
</html>
